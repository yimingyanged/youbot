<launch>
 <arg name="moveit_controller_manager" default="moveit_simple_controller_manager/MoveItSimpleControllerManager"/>
 <param name="moveit_controller_manager" value="$(arg moveit_controller_manager)"/>
 
 <arg name="controller_manager_name" default="youbot_ed_controller_manager" />
 <param name="controller_manager_name" value="$(arg controller_manager_name)" />
 
 <arg name="use_controller_manager" default="true" />
 <param name="use_controller_manager" value="$(arg use_controller_manager)" />
 <rosparam file="$(find youbot_ed_moveit_generated)/config/controllers.yaml"/>
</launch>
