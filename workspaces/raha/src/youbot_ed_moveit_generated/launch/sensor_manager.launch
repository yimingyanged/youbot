<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->  

  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="" />
  <include file="$(find youbot_ed_moveit_generated)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch" />
  
  <param name="octomap_frame" type="string" value="odom_combined" />
  <param name="octomap_resolution" type="double" value="0.05" />
  <param name="max_range" type="double" value="5.0" />
  <rosparam command="load" file="$(find youbot_ed_moveit_generated)/config/sensors_rgbd.yaml" />
  
</launch>
